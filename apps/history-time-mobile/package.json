{
  "name": "@history-time/mobile",
  "version": "0.1.0",
  "private": true,
  "main": "./index.js",
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:e2e": "node e2e/run-e2e-tests.js",
    "test:e2e:report": "node e2e/run-e2e-tests.js --report",
    "test:single": "node e2e/run-e2e-tests.js",
    "maestro:studio": "maestro studio",
    "lint": "eslint . --max-warnings=300",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check ."
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@expo/webpack-config": "^19.0.0",
    "@history-time/auth": "workspace:*",
    "@history-time/api-client": "workspace:*",
    "@history-time/config": "workspace:*",
    "@history-time/core-game": "workspace:*",
    "@history-time/types": "workspace:*",
    "@history-time/ui": "workspace:*",
    "@history-time/utils": "workspace:*",
    "@history-time/validation": "workspace:*",
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-native/normalize-color": "^2.1.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@types/uuid": "^10.0.0",
    "apisauce": "^3.0.1",
    "expo": "~49.0.15",
    "expo-image": "~1.3.5",
    "expo-image-picker": "~14.3.2",
    "expo-status-bar": "~1.6.0",
    "fbjs": "^3.0.5",
    "history-time-shared": "workspace:*",
    "inline-style-prefixer": "^6.0.1",
    "memoize-one": "^6.0.0",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "mobx-state-tree": "^5.4.1",
    "nullthrows": "^1.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "~0.19.6",
    "scheduler": "^0.26.0",
    "svg-url-loader": "^8.0.0",
    "svgo-loader": "^4.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-typescript": "^7.27.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expo": "^49.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-native-reanimated|react-native-gesture-handler|react-native-paper)"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "\\.todo\\.test\\.[tj]sx?$"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
